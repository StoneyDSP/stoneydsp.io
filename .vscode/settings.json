{

  "files.associations": {},
  "files.enableTrash": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherInclude": [],
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.hg/store/**": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true
  },

  "editor.insertSpaces": true,
  "editor.renderFinalNewline": "dimmed",

  "eslint.enable": true,
  "eslint.validate": [
    "javascript", // ...
    "typescript",
    "html" // Add "html" to enable linting `.html` files.
  ],
  "eslint.nodeEnv": null,
  "eslint.nodePath": null,
  "eslint.lintTask.enable": true,
  "eslint.lintTask.options": ".",
  "eslint.packageManager": "yarn",

  "css.format.enable": true,
  "css.format.newlineBetweenRules": true,
  "css.format.newlineBetweenSelectors": true,
  "css.format.braceStyle": "collapse",
  "css.validate": true,

  "css.lint.zeroUnits": "warning",
  "css.lint.argumentsInColorFunction": "warning",
  "css.lint.boxModel": "warning",
  "css.lint.compatibleVendorPrefixes": "warning",
  "css.lint.duplicateProperties": "warning",
  "css.lint.emptyRules": "warning",
  "css.lint.float": "warning",
  "css.lint.fontFaceProperties": "warning",
  "css.lint.hexColorLength": "warning",
  "css.lint.idSelector": "warning",
  "css.lint.ieHack": "warning",
  "css.lint.important": "warning",
  "css.lint.importStatement": "warning",
  "css.lint.propertyIgnoredDueToDisplay": "warning",
  "css.lint.universalSelector": "warning",
  "css.lint.unknownAtRules": "warning",
  "css.lint.unknownProperties": "warning",
  "css.lint.unknownVendorSpecificProperties": "warning",
  "css.lint.vendorPrefix": "warning",

  "html.format.enable": true,
  "html.validate.styles": true,
  "html.validate.scripts": true,

  "nuxt.isNuxtApp": false,
  "npm.autoDetect": "on",

  "git.autofetch": false,
  "git.autorefresh": false,
  "git.enabled": true,
  "git.mergeEditor": true,
  "github.gitAuthentication": true,
  "github.gitProtocol": "ssh",

  "markdown.math.enabled": true,
  "markdown.validate.enabled": true,
  "markdown.validate.fileLinks.enabled": "error",

  "json.format.enable": true,
  "json.schemaDownload.enable": true,
  "json.validate.enable": true,
  "json.schemas": [
    {
      "fileMatch": [ "tsconfig.json" ],
      "schema": true,
      "url": "https://json.schemastore.org/tsconfig"
    }
  ],

  "typescript.check.npmIsInstalled": true,
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.format.enable": true,
  "typescript.locale": "en",
  "typescript.tsc.autoDetect": "on",
  "typescript.validate.enable": true,
  "typescript.tsdk": "~/.config/yarn/global/node_modules/typescript/lib",
  "typescript.suggest.paths": true,
  "typescript.suggest.enabled": true,

  "[html]": {
    "editor.suggest.insertMode": "replace"
  },

  "[css]": {
    "editor.suggest.insertMode": "replace"
  },

  "[javascript]": {
    "editor.maxTokenizationLineLength": 2500
  },

  "[typescript]": {},

  "[markdown]": {
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.wordWrap": "on",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    }
  },


  "[json]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace"
  }
}
